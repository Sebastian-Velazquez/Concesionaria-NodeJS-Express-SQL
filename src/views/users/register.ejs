<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../parcial/head.ejs') %>
    <link href="/css/register.css" rel="stylesheet">
</head>
<body>
    
    <%- include('../parcial/header.ejs') %>
    <main>
        
        <h1 class="content-main__div-h1">Registro de usuario</h1>
        
        <form action="#" method="#" class="content-main__form">
                    
                    <input type="text" name="nombreUsuario" placeholder="NOMBRE USUARIO" class="form-input">
                    <!-- <h5></h5> -->
                    <input type="password" placeholder="CONTRASEÑA" name="password" class="form-input">
                    <!-- <h5>CONFIRMAR CONTRASEÑA</h5> -->
                    <input type="password" placeholder="CONFIRMAR CONTRASEÑA" name="passwordConfirmar" class="form-input">
            <h2>Datos personales</h2>    
                    <!-- <h5></h5> -->
                    <input type="text" placeholder="NOMBRE / RAZON SOCIAL" name="nombre-razonSocial" class="form-input">
                    <!-- <h5></h5> -->
                    <input type="email" placeholder="E-MAIL" name="email" class="form-input">
                    <!-- <h5></h5> -->
                    <input type="email" placeholder="CONFIRMAR E-MAIL" name="emailConfirmar" class="form-input"> 
                    <!-- <h5></h5> -->
                    <input type="text" placeholder="DIRECCION" name="direccion" class="form-input">
                    <!-- h5></h5> -->
                    <input type="number" placeholder="TELEFONO MOVIL" neme="telefono" class="form-input">
            <div class="form-button-content">
                <button type="submit" class="form-button">REGISTRARSE</button>
            </div>
        </form>
    </main>
    <%- include('../parcial/footer.ejs') %>
</body>
</html>